   "source": [
    "from cvxopt import matrix,solvers\n",
    "\n",
    "P = eye(d+1)\n",
    "P[0,0] = 0\n",
    "P = matrix(P)\n",
    "\n",
    "q = zeros(d+1)\n",
    "q = matrix(q)\n",
    "\n",
    "G = (-X*y).T\n",
    "G = matrix(G)\n",
    "\n",
    "h = -ones(n)\n",
    "h = matrix(h)"
   ]
